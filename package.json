{
  "name": "digitalgov-workflow-react",
  "version": "1.0.0",
  "description": "A site for managing the Digital.gov editorial workflow on GitHub.",
  "scripts": {
    "dev": "ENDPOINT=http://localhost:4000/ next",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "link:unix": "ln -s $(pwd)/node_modules/uswds/dist/fonts/ $(pwd)/static/fonts && ln -s $(pwd)/node_modules/uswds/dist/img/ $(pwd)/static/images",
    "link:windows": "mklink /J .\\node_modules\\uswds\\dist\\fonts\\ .\\static\\fonts\\ && mklink /J .\\node_modules\\uswds\\dist\\img\\ .\\static\\images\\",
    "netlify": "cp -r ./node_modules/uswds/dist/img ./static/images/ && cp -r ./node_modules/uswds/dist/fonts ./static/fonts && next build && next export",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "main": "pages/index.js",
  "author": "Felix Mattick",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@zeit/next-sass": "^1.0.1",
    "apollo-client": "^2.4.8",
    "apollo-link-http": "^1.5.9",
    "builtin-status-codes": "^3.0.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "html-entities": "^1.2.1",
    "moment": "^2.23.0",
    "next": "^7.0.2",
    "next-apollo": "^2.0.6",
    "next-seo": "^1.8.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.7.0",
    "react-tagsinput": "^3.19.0",
    "uswds": "2.0.0-beta.4",
    "webpack": "^4.28.1"
  },
  "repository": "https://github.com/arch-lord/digitalgov-workflow-react.git",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-plugin-react": "^7.12.3"
  }
}
